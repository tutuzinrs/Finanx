{
  "expo": {
    "name": "Finax",
    "slug": "Finax",
    "scheme": "finax",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoIcon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logoIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#25b05f"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoIcon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "statusBar": {
        "translucent": true,
        "backgroundColor": "#00000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "O aplicativo acessa suas fotos para permitir que vocÃª escolha uma foto de perfil."
        }
      ]
    ]
  }
}
